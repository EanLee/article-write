{
  "name": "blog-writing-app",
  "version": "1.0.0",
  "description": "A blog writing application integrating Obsidian markdown editing with Astro blog publishing",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json && electron dist/main/main.js",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix"
  },
  "keywords": [
    "electron",
    "vue",
    "typescript",
    "blog",
    "obsidian",
    "astro"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.0.0",
    "daisyui": "^3.9.0",
    "electron": "^25.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-electron": "^0.12.0",
    "vitest": "^0.34.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/markdown-it": "^13.0.0",
    "chokidar": "^3.5.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.0",
    "pinia": "^2.1.0",
    "vue": "^3.3.0"
  }
}